# API Keys for Gemini/Kimi Agent Integration
# Copy this file to .env and fill in your actual keys

# Gemini API Key (Google Generative AI)
# Get your key at: https://makersuite.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# Kimi API Key (Moonshot AI)
# Get your key at: https://platform.moonshot.ai/
KIMI_API_KEY_v3=your_kimi_api_key_here

# Optional: MLflow tracking URI
# MLFLOW_TRACKING_URI=http://localhost:5000

# Optional: Redis connection
# REDIS_HOST=localhost
# REDIS_PORT=6379

# --- Neon Specialist Agent ---

# Neon PostgreSQL (REQUIRED for neon-docs)
PRJ_NEON_DATABASE_URL=postgresql://user:pass@ep-xxx.us-west-2.aws.neon.tech/neondb?sslmode=require

# Ollama (defaults shown)
OLLAMA_HOST=http://localhost:11434
OLLAMA_EMBED_MODEL=nomic-embed-text

# MLflow (optional - overrides MLFLOW_TRACKING_URI above if set)
MLFLOW_EXPERIMENT_NAME=pm-neon-docs

# Logging
LOG_LEVEL=INFO
